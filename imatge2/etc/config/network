interface 'loopback'
        option ifname 'lo'
        option proto 'static'
        option ipaddr '127.0.0.1'
        option netmask '255.0.0.0'


config interface 'lan'
	option type 'bridge'
	option auto '1'
	option proto 'static'
	option ipaddr '170.30.2.3'
	option netmask '255.255.255.0'
	option ip6addr 'fc01:1934:1:1d20::3/60'

config interface 'meshlocal'
	option type 'static'
	option ifname 'eth0.5'
	
